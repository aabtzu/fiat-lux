lex;\n    flex-direction: column;\n    justify-content: center;\n    text-align: center;\n    overflow: hidden;\n  }\n\n  .class-block:hover {\n    transform: translateY(-2px);\n    box-shadow: 0 4px 12px rgba(108, 117, 125, 0.3);\n    background: #5a6268;\n  }\n\n  .class-code {\n    font-weight: 700;\n    font-size: 0.7rem;\n    margin-bottom: 0.2rem;\n    white-space: nowrap;\n    overflow: hidden;\n    text-overflow: ellipsis;\n  }\n\n  .class-title {\n    font-size: 0.6rem;\n    line-height: 1.1;\n    font-weight: 400;\n    overflow: hidden;\n    text-overflow: ellipsis;\n    display: -webkit-box;\n    -webkit-line-clamp: 2;\n    -webkit-box-orient: vertical;\n  }\n\n  /* Time slot positioning */\n  .time-0830 { top: 0px; }\n  .time-0950 { top: 80px; }\n  .time-1030 { top: 120px; }\n  .time-1145 { top: 195px; }\n  .time-1335 { top: 315px; }\n\n  /* Different heights for different duration classes */\n  .duration-65 { height: 55px; }\n  .duration-100 { height: 85px; }\n  .duration-210 { height: 180px; }\n\n  /* Adjust font sizes based on block height */\n  .duration-65 .class-code { font-size: 0.65rem; }\n  .duration-65 .class-title { font-size: 0.55rem; -webkit-line-clamp: 1; }\n  \n  .duration-100 .class-code { font-size: 0.7rem; }\n  .duration-100 .class-title { font-size: 0.6rem; -webkit-line-clamp: 2; }\n  \n  .duration-210 .class-code { font-size: 0.8rem; }\n  .duration-210 .class-title { font-size: 0.7rem; -webkit-line-clamp: 3; }\n\n  .summary {\n    background: #fff;\n    border-radius: 8px;\n    padding: 1rem;\n    margin-top: 2rem;\n    text-align: center;\n    border: 1px solid #e9ecef;\n  }\n\n  .summary-stats {\n    display: flex;\n    justify-content: center;\n    gap: 2rem;\n    flex-wrap: wrap;\n  }\n\n  .stat-item {\n    text-align: center;\n  }\n\n  .stat-number {\n    font-size: 1.5rem;\n    font-weight: 700;\n    color: #6c757d;\n  }\n\n  .stat-label {\n    font-size: 0.8rem;\n    color: #6c757d;\n    text-transform: uppercase;\n    letter-s